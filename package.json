{
  "name": "@minimal/minimal-kit-react",
  "author": "minimals.cc",
  "version": "3.3.0",
  "description": "Simple React Scripts & Javascript",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx , .tsx, .ts ",
    "lint:fix": "eslint --fix --ext .js,.jsx, .tsx, .ts",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@iconify/react": "^3.1.4",
    "@mui/material": "^5.5.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "change-case": "^4.1.2",
    "framer-motion": "^6.2.8",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "react-lazy-load-image-component": "^1.5.1",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.0",
    "simplebar": "^5.3.6",
    "simplebar-react": "^2.3.6",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  }
}
